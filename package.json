{
  "name": "ssr-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch:client": "cross-env webpack-dev-server --hot --mode development --output-pathinfo --inline --source-map --config configs/webpack.config.js",
    "build:client": "NODE_ENV=production cross-env webpack --mode production --config configs/webpack.config.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.4",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/serve-static": "^1.0.2",
    "@nestjs/typeorm": "^6.3.1",
    "@storybook/addon-storyshots": "^5.3.13",
    "@storybook/addon-storyshots-puppeteer": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/flat": "^0.0.28",
    "@types/history": "^4.7.4",
    "@types/keymaster": "^1.6.28",
    "@types/lodash.throttle": "^4.1.6",
    "@types/puppeteer": "^2.0.0",
    "@types/react": "^16.9.19",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-dom": "^16.9.5",
    "@types/react-virtualized": "^9.21.8",
    "apollo-server-core": "^2.10.1",
    "apollo-server-express": "^2.10.1",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "dom-align": "^1.10.4",
    "flat": "^5.0.0",
    "graphql": "^14.6.0",
    "history": "^4.10.1",
    "jsdom": "^16.0.1",
    "jss": "^10.0.3",
    "keymaster": "^1.6.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.snakecase": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.4",
    "mobx-utils": "^5.5.3",
    "natural-orderby": "^2.0.3",
    "path-to-regexp": "^6.1.0",
    "puppeteer": "^2.1.1",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-imask": "^6.0.1",
    "react-jss": "^10.0.3",
    "react-virtualized": "^9.21.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.22",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^13.7.7",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
